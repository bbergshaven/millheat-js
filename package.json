{
  "name": "millheat-js-api",
  "version": "0.0.1",
  "description": "Access millheats API with ease using our API client",
  "main": "dist/millheat-js.cjs",
  "module": "dist/millheat-js.mjs",
  "typings": "dist/millheat-js.d.ts",
  "exports": {
    ".": {
      "require": "./dist/millheat-js.cjs",
      "import": "./dist/millheat-js.mjs",
      "types": "./dist/millheat-js.d.ts"
    }
  },
  "scripts": {
    "build": "rollup -c"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/bbergshaven/millheat-js.git"
  },
  "keywords": [
    "millheat",
    "iot",
    "iiot"
  ],
  "peerDependencies": {
    "node-fetch": "^2"
  },
  "peerDependenciesMeta": {
    "node-fetch": {
      "optional": true
    }
  },
  "author": "Bernt-Johan Bergshaven // Searis AS",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bbergshaven/millheat-js/issues"
  },
  "devDependencies": {
    "@types/node-fetch": "^2.6.1",
    "esbuild": "^0.14.28",
    "msw": "^0.41.0",
    "node-fetch": "^2",
    "rollup": "^2.70.1",
    "rollup-plugin-dts": "^4.2.0",
    "rollup-plugin-esbuild": "^4.8.2",
    "ts-node": "^10.7.0",
    "tslib": "^2.3.1",
    "typescript": "4.7"
  },
  "publishConfig": {
    "access": "public"
  },
  "volta": {
    "node": "16.14.2",
    "npm": "8.11.0"
  },
  "dependencies": {
    "abort-controller": "^3.0.0"
  }
}
